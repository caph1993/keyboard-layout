;; nop0 is the "go to extreme" button <=>
;; nop3 is a "ctl" button. All ctl buttons trigger nop3, except the tab key for which certain keys are unmoded. 
;; Repeated arrow means C-arrow (word jump). Only defined for left and right.
(defalias <=> nop0)
(defalias ‚Üê (fork left  home   (nop0)))
(defalias ‚Üë (fork up    pgup   (nop0)))
(defalias ‚Üì (fork down  pgdn   (nop0)))
(defalias ‚Üí (fork right end    (nop0)))
(defalias ‚Üê‚Üê (fork C-left (unmod (lctl rctl) home) (lctl rctl ctl nop0)))
(defalias ‚Üí‚Üí (fork C-right (unmod (lctl rctl) end) (lctl rctl ctl nop0)))
(defalias bspc (fork bspc (macro S-home (unmod bspc)) (nop0)))
(defalias del  (fork del  (macro S-end  (unmod del) ) (nop0)))
(defalias esc  (multi esc (on-press release-vkey vkey-layouts)))

